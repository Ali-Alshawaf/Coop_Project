@model CoopProj.Models.Users

@{
    ViewData["Title"] = "CreateSupervisor";
}

<div class="main  align-items-center">
    <h2>Add Supervisor</h2>
    <div class="row  ">

    <div class="col-lg-8">
        <div class="card">
            <div class="card-body">
                    <a href="javascript:void(0);" onclick="window.history.back();" style="font-size:35px; "><i class="fe-arrow-left"></i></a>

                <hr style="border-top: 2px solid #1abc9c; font-weight: bold;">
                <form asp-action="CreateSupervisor">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="Name" class="control-label"></label>
                        <input asp-for="Name" class="form-control" />
                        <span asp-validation-for="Name" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Email" class="control-label"></label>
                        <input asp-for="Email" class="form-control" />
                        <span asp-validation-for="Email" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="NumberPhone" class="control-label"></label>
                        <input asp-for="NumberPhone" class="form-control" />
                        <span asp-validation-for="NumberPhone" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PassWord" class="control-label"></label>
                        <input asp-for="PassWord" class="form-control" />
                        <span asp-validation-for="PassWord" class="text-danger"></span>
                    </div>
                    <div class="form-group mt-4">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>
    </div>
    </div>
    </div>

